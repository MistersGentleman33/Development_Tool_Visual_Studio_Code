{
	"name": "Web-Translate",
	"displayName": "Web Translate",
	"description": "Find In Browser",
	"version": "1.0.5",
	"publisher": "CriticalSoft",
	"engines": {
		"vscode": "^1.31.0"
	},
	"author": {
		"name": "Critical Soft",
		"url": "https://www.github.com/criticalsoft"
	},
	"icon": "resources/icon.png",
	"homepage": "https://www.github.com/criticalsoft",
	"repository": {
		"type": "git",
		"url": "https://www.github.com/criticalsoft"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "extension.WebTranslate",
					"group": "web@26"
				}
			]
		},
		"commands": [
			{
				"command": "extension.WebTranslate",
				"title": "Translate"
			}
		],
		"keybindings": [
			{
				"command": "extension.WebTranslate",
				"key": "ctrl+alt+f t",
				"when": "editorHasSelection"
			}
		],
		"configuration": {
			"title": "Config",
			"type": "object",
			"properties": {
				"WebTranslate.QueryTemplate": {
					"type": "string",
					"default": "https://translate.google.com/?text=%STRING%",
					"description": "URL"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"__metadata": {
		"id": "4a3717df-00d2-40dd-9ed5-cea39d4d6z26",
		"publisherId": "096d9204-8dd5-45b0-9611-292ee7bafz26",
		"publisherDisplayName": "",
		"installedTimestamp": 1619096124034
	}
}
